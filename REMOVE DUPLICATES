import java.util.Arrays;
import java.util.HashSet;

public class RemoveDuplicates {
    public static void main(String[] args) {
        // Example array with duplicates
        Integer[] arrayWithDuplicates = {1, 2, 3, 2, 4, 5, 1, 6, 4};

        // Display original array
        System.out.println("Original array: " + Arrays.toString(arrayWithDuplicates));

        // Remove duplicates using a HashSet
        HashSet<Integer> set = new HashSet<>(Arrays.asList(arrayWithDuplicates));

        // Convert the HashSet back to an array
        Integer[] arrayWithoutDuplicates = set.toArray(new Integer[0]);

        // Display array without duplicates
        System.out.println("Array without duplicates: " + Arrays.toString(arrayWithoutDuplicates));
    }
}
